    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
            "justMyCode": true
        },
        {
            "name": "backend",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backends.routers.home:app", // Replace with your module name if different
                "--host",
                "127.0.0.1",
                "--port",
                "8080",
                "--reload", // Optional: enables auto-reload on code changes
                "--reload-dir",
                "${workspaceFolder}/backends"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
            "justMyCode": true
        },
